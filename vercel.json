{
  "version": 2,
  "builds": [
    {
      "src": "hayf/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "hayf/wsgi.py"
    }
  ],
  "env": {
    "VERCEL": "1",
    "PYTHONUNBUFFERED": "1"
  }
}
